# Definindo a fonte padrão de gems
source 'https://rubygems.org'

# Dependência para testes BDD com Cucumber
gem 'cucumber'

# Dependência para interagir com APIs RESTful usando HTTP
gem 'httparty'

# Dependência para testes unitários com RSpec
gem 'rspec'

# Dependência para gerar relatórios de testes com Cucumber e RSpec
gem 'report_builder'
